<!DOCTYPE html>
<html>
  <head>
    <title>Room Review</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link href="/app.css" rel="stylesheet">
  </head>
  <body>

      <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">

    <div class="navbar-wrapper">
      <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <img id="logo" class="navbar-brand" class="img-responsive" src="images/logo.png" alt="logo">
          </div>

          <form action="/searchResults" method="get">
            <div class="nav navbar-nav">
              <div class="navbar-form" role="search">
                <div class="input-group">
                  <input id="searchwidth" type="text" class="form-control" style="col-xs-4" placeholder="Search" name="search">
                  <div class="input-group-btn">
                    <button class="btn btn-default" type="submit"><span class="glyphicon glyphicon-search" id="glyph"></span></button>
                  </div>
                </div>
              </div>
            </div>
          </form>

          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="active"><a href="/">Home</a></li>
              <li><a href="/aboutUs">About us</a></li>
              <li><a href="/contact">Contact us</a></li>
            </ul>
          </div>
        </div>
      </nav>
    </div>

    <div class="container">

      #parse( $template )

    </div>
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
  </body>
</html>
